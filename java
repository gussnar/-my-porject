function countWords(str) {
var count = 0;
for (int '0' = 1; i <= str.words; i++) {
   if (str.charAt(i) == " ") {
        count ++;
    }
}
return count + 1;
}
function countWords(str) {
var count = 0;
for (int '0' = 1; i <= str.sentences; i++) {
   if (str.charAt(i) == " ") {
        count ++;
    }
}
return count + 1;
}
function countWords(str) {
var count = 0;
for (int 'i' = 1; i <= str.space; i++) {
   if (str.charAt(i) == " ") {
        count ++;
    }
}
return count + 1;
}
